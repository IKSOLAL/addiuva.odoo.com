<?xml version="1.0"?>
<t t-name="ikatech_invoice.report_invoice_document_ikatech">
        <t t-call="ikatech_invoice.ikatech_external_layout">
            <t t-set="o" t-value="o.with_context({'lang':o.partner_id.lang})"/>
            <t t-set="document_letter" t-value="o.l10n_latam_document_type_id.l10n_ar_letter"/>
            <t t-set="document_legend" t-value="o.l10n_latam_document_type_id.code and 'Cod. %02d' % int(o.l10n_latam_document_type_id.code) or ''"/>
            <t t-set="report_name" t-value="o.l10n_latam_document_type_id.report_name"/>

            <div class="page" style="padding-top: -10px; font-size:18px;">
                <div class="row">
                    <!--INICIA LOGO DE LA EMPRESA-->
                    <div class="col-3">
                        <img t-if="o.company_id.logo" t-att-src="'data:image/png;base64,%s' % to_text(o.company_id.logo)" style="max-height: 180px;max-width: 180px;"/>
                    </div>
                    <!--TERMINA LOGO DE LA EMPRESA-->

                    <!--EMPIEZA DATOS DEL EMISOR-->
                    <div class="col-5">
                        <center>
                            <strong>
                                <span t-field="o.company_id.name" style="font-size:16px"/>
                            </strong>
                            <div style="font-size:11px">
                                <span t-field="o.company_id.street_name"/>
                                &amp;nbsp;
                                <div t-if="o.company_id.street_number">
                                    <span>No Ext.</span>
                                    &amp;nbsp;
                                    <span t-field="o.company_id.street_number"/>
                                </div>
                                pager
                                &amp;nbsp;
                                <div t-if="o.company_id.street_number2">
                                    <span>No Int.</span>
                                    &amp;nbsp;
                                    <span t-field="o.company_id.street_number2"/>
                                </div>
                                &amp;nbsp;
                                <div t-if="o.company_id.l10n_mx_edi_colony">
                                    <span>Colonia</span>
                                    &amp;nbsp;
                                    <span t-field="o.company_id.l10n_mx_edi_colony"/>
                                </div>
                                &amp;nbsp;
                                <t t-if="o.company_id.zip">
                                    <span>CP.</span>
                                    &amp;nbsp;
                                    <span t-field="o.company_id.zip"/></t>,
                                &amp;nbsp;
                                <span t-field="o.company_id.city"/>,
                                &amp;nbsp;
                                <span t-field="o.company_id.state_id.name"/>
                                &amp;nbsp;
                                <span t-field="o.company_id.country_id.name"/>
                                <br/>
                                <span>Telefono:</span>
                                &amp;nbsp;
                                <span t-field="o.company_id.phone"/>
                                <br/>
                                &amp;nbsp;
                                <span>Sitio Web:&amp;nbsp;</span>
                                <spant t-field="o.company_id.website"/>
                                <br/>
                                <t t-if="o.company_id.country_id.code=='AR'">
                                    <span>
                                        <div name="center-upper" class="col-2 text-center" t-att-style="'color: %s;' % o.company_id.primary_color">
                                            <span style="display: inline-block; text-align: center; line-height: 8px;">
                                                <h1 style="line-height: 35px;">
                                                    <strong>
                                                        <span t-esc="not pre_printed_report and document_letter or ' '"/>
                                                    </strong>
                                                </h1>
                                                <span style="font-size: x-small;" t-esc="not pre_printed_report and document_legend or ' '"/>
                                            </span>
                                        </div>
                                    </span>
                                </t>


                            </div>
                        </center>
                    </div>
                    <div class="col-4" style="border: 2px; border-style: groove;border-radius: 10px;">
                        <div style="padding:30px 20px 20px 20px;">
                            <center>
                                <t t-if="o.company_id.country_id.code=='MX'">
                                    <strong>
                                        <span>R.F.C</span>
                                    </strong>
                                     <span t-field="o.company_id.vat"/>
                                </t>
                                <t t-if="o.company_id.country_id.code=='CO'">
                                    <strong>
                                        <span>NIT</span>
                                    </strong>
                                     <span t-field="o.company_id.vat"/>
                                </t>
                                <t t-if="o.company_id.country_id.code=='CL'">
                                    <strong>
                                        <span>RUT</span>
                                    </strong>
                                     <span t-field="o.company_id.vat"/>
                                </t>
                                <t t-if="o.company_id.country_id.code=='CR'">
                                    <strong>
                                        <span style="font-size:20px" t-esc="o.company_id.identification_id.name"/>
                                    </strong>
                                     <span t-field="o.company_id.vat"/>
                                </t>
                                <t t-if="o.company_id.country_id.code=='AR'">
                                    <strong>
                                        <span>CUIT:</span>
                                    </strong>
                                    <span t-field="o.company_id.vat"/>
                                </t>
                                <br/>
                                <span t-if="o.move_type == 'out_invoice' and o.state == 'posted' and o.company_id.country_id.code=='MX'">
                                    <strong>
                                        <span>FACTURA ELECTRONICA</span>
                                    </strong>
                                    <br/>
                                </span>
                                <span t-if="o.move_type == 'out_invoice' and o.state == 'posted' and o.company_id.country_id.code=='CO'">
                                    <strong>
                                        <span>FACTURA ELECTRONICA DE VENTA</span>
                                    </strong>
                                    <br/>
                                </span>
                                <span t-if="o.move_type == 'out_invoice' and o.state == 'posted' and o.company_id.country_id.code=='CR'">
                                    <strong>
                                        <t t-esc="dict (o.fields_get (allfields = ['tipo_documento']) ['tipo_documento'] ['selection']) [o.tipo_documento]"/>
                                    </strong>
                                    <br/>
                                </span>
                                <span t-if="o.move_type == 'out_invoice' and o.state == 'posted' and o.company_id.country_id.code=='AR'">
                                    <div name="right-upper-side" class="col-8 text-right" style="padding-left: 0px;" t-if="not pre_printed_report">
                                        <!-- (6) Titulo de Documento DICE FACTURA -->
                                        <h4 t-att-style="'color: %s;' % o.company_id.primary_color">
                                            <strong style="font-size:18px;color:#000000;">
                                                <span t-esc="o.l10n_latam_document_type_id.report_name"/>
                                            </strong>
                                        </h4>

                                    </div>
                                </span>
                                <span t-if="o.move_type == 'out_invoice' and o.state == 'draft'">
                                    <strong>
                                        <span>FACTURA BORRADOR</span>
                                    </strong>
                                    <br/>
                                </span>
                                <span t-if="o.move_type == 'out_invoice' and o.state == 'cancel'">
                                    <strong>
                                        <span>FACTURA CANCELADA</span>
                                    </strong>
                                    <br/>
                                </span>
                                <span t-if="o.move_type == 'out_refund'">
                                    <strong>
                                        <span>NOTA DE CREDITO</span>
                                    </strong>
                                    <br/>
                                </span>
                                <span t-if="o.move_type == 'in_refund'">
                                    <strong>
                                        <span>NOTA DE CREDITO PROVEEDOR</span>
                                    </strong>
                                    <br/>
                                </span>
                                <span t-if="o.move_type == 'in_invoice'">
                                    <strong>
                                        <span>FACTURA PROVEEDOR</span>
                                    </strong>
                                    <br/>
                                </span>
                                <span t-if="o.company_id.country_id.code=='MX'">
                                    <span>FOLIO:</span>
                                </span>
                                <span t-if="o.company_id.country_id.code=='CO'">
                                    <span>NÚMERO:</span>
                                </span>
                                &amp;nbsp;
                                 <t t-if="o.company_id.country_id.code=='AR'">
                                     <t t-if="not pre_printed_report">
                                        <!-- (7) Numero punto venta - (8) numero de documento -->
                                        <span t-att-style="'color: %s;' % o.company_id.secondary_color">Nro</span>
                                        <span t-esc="o.l10n_latam_document_number"/>
                                    </t>
                                 </t>
                                <t t-if="o.company_id.country_id.code != 'AR'">
                                    <span t-if="o.name != '/'" t-field="o.name"/>
                                </t>
                                <span t-if="o.company_id.country_id.code=='AR'">
                                    <span><br/>
                                        <span t-att-style="'color: %s;font-size:18px;' % o.company_id.secondary_color">
                                            IIBB
                                        </span><br/>
                                        <span style="font-size:18px;color:black;" t-esc="o.company_id.l10n_ar_gross_income_type == 'exempt' and 'Exento' or o.company_id.l10n_ar_gross_income_number"/>
                                        <br/>
                                        <span t-att-style="'color: %s; font-size:18px;' % o.company_id.secondary_color">
                                            Inicio de actividades
                                        </span>
                                        <span style="font-size:18px;color:black;" t-field="o.company_id.l10n_ar_afip_start_date" t-options="{&quot;format&quot;: &quot;dd/MM/yyyy&quot;}"/>
                                    </span>
                                </span>
                                <br/>
                            </center>
                        </div>
                    </div>
                    <!--TERMINA DATOS DEL EMISOR-->
                </div>
                <br/>
                <div class="row" style="border: 2px; border-style: groove;border-radius: 10px;" name="info_row">
                    <div class="col-7" style="font-size:12px">
                        <table style="border-spacing: 1px; border-collapse: separate;">
                            <tr>
                                <td style="width:24% !important;">
                                    <strong>
                                            <span>FECHA</span>
                                    </strong>
                                </td>
                                <td>
                                   :&amp;nbsp;<span t-field="o.invoice_date" t-options="{&quot;format&quot;: &quot;dd/MM/yyyy&quot;}"/>

                                </td>
                                </tr>
                            <tr>
                                <td>
                                    <strong>
                                        <span>CLIENTE</span>
                                    </strong>
                                </td>
                                <td>:&amp;nbsp;
                                    <span t-field="o.partner_id.name"/>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <strong>
                                        <span>DIRECCIÓN</span>
                                    </strong>
                                </td>
                                <td>:&amp;nbsp;<span t-field="o.partner_id.street"/>, &amp;nbsp;
                                    <span t-field="o.partner_id.l10n_mx_edi_colony"/>,
                                    alcaldía <span t-field="o.partner_id.city_id.name"/>,
                                    <span t-field="o.partner_id.state_id.name"/>,
                                    CP <span t-field="o.partner_id.zip"/>

                                </td>
                            </tr>
                            <!-- TODO: REVISAR PQ NO TIENE NADA
                            <tr>
                                <td>
                                    <strong>
                                        <span>CONTACTO: &amp;nbsp;</span>
                                    </strong>
                                </td>
                            </tr>
                            -->
                            <tr t-if="o.company_id.country_id.code=='AR'">
                                <td>
                                    <strong>
                                        <span>COND. IVA</span>
                                    </strong>
                                </td>
                                <td>
                                    :&amp;nbsp;<span t-field="o.partner_id.l10n_ar_afip_responsibility_type_id"/>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div class="col-1"/>
                    <div class="col-4" style="font-size:12px">
                        <table>
                            <tr>
                                <td>
                                    <t t-if="o.company_id.country_id.code=='MX'">
                                        <strong>
                                            <span>R.F.C</span>
                                        </strong>
                                        <td>:&amp;nbsp;
                                            <span t-field="o.partner_id.vat"/>
                                        </td>
                                    </t>
                                    <t t-if="o.company_id.country_id.code=='CO'">
                                        <strong>
                                            <span>NIT</span>
                                        </strong>
                                        <td>:&amp;nbsp;
                                            <span t-field="o.partner_id.vat"/>
                                        </td>
                                    </t>
                                    <t t-if="o.company_id.country_id.code=='CL'">
                                        <strong>
                                            <span>RUT</span>
                                        </strong>
                                        <td>:&amp;nbsp;
                                            <span t-field="o.partner_id.vat"/>
                                        </td>
                                    </t>
                                    <t t-if="o.company_id.country_id.code=='CR'">
                                        <strong>
                                            <span t-esc="o.partner_id.identification_id.name" style="text-transform: uppercase;"/>
                                        </strong>
                                        <td>:&amp;nbsp;
                                            <span t-field="o.partner_id.vat"/>
                                        </td>
                                    </t>
                                    <t t-if="o.company_id.country_id.code=='AR'">
                                        <strong>
                                            <span>CUIT</span>
                                        </strong>
                                        <td>:&amp;nbsp;
                                            <span t-field="o.partner_id.vat"/>
                                        </td>
                                    </t>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <strong>
                                        <span>CIUDAD</span>
                                    </strong>
                                </td>
                                <td>:&amp;nbsp;
                                    <span t-field="o.partner_id.city"/>
                                </td>
                            </tr>

                            <tr>
                                <td>
                                    <strong>
                                        <span>VENCIMIENTO</span>
                                    </strong>
                                </td>
                                <td>:&amp;nbsp;
                                    <span t-field="o.invoice_date_due" t-options="{&quot;format&quot;: &quot;dd/MM/yyyy&quot;}"/>
                                </td>
                            </tr>


                            <tr t-if="o.company_id.country_id.code=='MX'">
                                <td>
                                    <strong>
                                        <span>FORMA DE PAGO</span>
                                        <br/>
                                    </strong>
                                </td>
                                <td>:&amp;nbsp;
                                    <span t-esc="'%s - %s' % (o.l10n_mx_edi_payment_method_id.code, o.l10n_mx_edi_payment_method_id.name)"/>
                                </td>
                            </tr>

                            <tr t-if="o.company_id.country_id.code=='AR'">
                                <t t-if="o.partner_id.email">
                                    <td>
                                        <strong>
                                            <span>EMAIL</span>
                                            <br/>
                                        </strong>
                                    </td>
                                    <td>:&amp;nbsp;
                                        <t t-esc="o.partner_id.email"/>
                                    </td>
                                </t>
                            </tr>

                        </table>
                    </div>
                </div>
                <br/>
                <div class="row" style="border: 2px; border-style: groove;border-radius: 10px;">
                    <table class="table" style="font-size:12px" name="invoice_line_table">
                        <thead class="thead-light text-center">
                            <tr>
                                <th style="width:40%; padding:0px 0px 0px 0px;color:black;">
                                    <strong>
                                        <span>Descripción</span>
                                    </strong>
                                </th>
                                <th style="width:10%; padding:0px 0px 0px 0px;color:black;">
                                    <strong>Cantidad</strong>
                                </th>
                                <th style="width:10%; padding:0px 0px 0px 0px;color:black;">
                                    <strong>Precio Unitario</strong>
                                </th>
                                <th style="width:10%; padding:0px 0px 0px 0px;color:black;">
                                    <strong>Dscto</strong>
                                </th>
                                <th style="width:10%; padding:0px 0px 0px 0px;color:black;">
                                    <strong>Total</strong>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-set="lines" t-value="o.invoice_line_ids.sorted(key=lambda l: (-l.sequence, l.date, l.move_name, -l.id), reverse=True)"/>
                            <t t-foreach="lines" t-as="line">
                                <tr t-att-class="'bg-200 font-weight-bold o_line_section' if line.display_type == 'line_section' else 'font-italic o_line_note' if line.display_type == 'line_note' else ''">
                                    <t t-if="not line.display_type" name="account_invoice_line_accountable">
                                        <td name="account_invoice_line_name">
                                            <span t-field="line.name" t-options="{'widget': 'text'}"/>
                                        </td>
                                        <td>
                                            <center>
                                                <span t-field="line.quantity"/>
                                                <span t-field="line.product_uom_id" groups="uom.group_uom"/>
                                            </center>
                                        </td>
                                        <td t-attf-class="{{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                            <center>
                                                <span class="text-nowrap" t-field="line.price_unit"/>
                                            </center>
                                        </td>
                                        <td t-attf-class="{{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                            <center>
                                                <!--<span class="text-nowrap" t-field="line.discount"/>-->
                                                <span t-esc="'%.2f'% line.discount"/>
                                            </center>
                                        </td>
                                        <td class="text-right o_price_total">
                                            <span class="text-nowrap" t-field="line.price_subtotal" groups="account.group_show_line_subtotals_tax_excluded"/>
                                            <span class="text-nowrap" t-field="line.price_total" groups="account.group_show_line_subtotals_tax_included"/>
                                        </td>
                                    </t>
                                    <t t-if="line.display_type == 'line_section'">
                                        <td colspan="99">
                                            <span t-field="line.name" t-options="{'widget': 'text'}"/>
                                        </td>
                                        <t t-set="current_section" t-value="line"/>
                                        <t t-set="current_subtotal" t-value="0"/>
                                    </t>
                                    <t t-if="line.display_type == 'line_note'">
                                        <td colspan="99">
                                            <span t-field="line.name" t-options="{'widget': 'text'}"/>
                                        </td>
                                    </t>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </div>
                <div class="clearfix" style="margin-top:10px">
                    <div id="total" class="row">
                        <div t-attf-class="#{'col-4' if report_type != 'html' else 'col-sm-7 col-md-4'} ml-auto" style="border-style: groove; border-radius: 10px;">
                            <table class="table table-sm" style="page-break-inside: avoid;font-size:13px">
                                <!--Tax totals-->
                                <t t-set="tax_totals" t-value="json.loads(o.tax_totals_json)"/>
                                <t t-foreach="tax_totals['subtotals']" t-as="subtotal">
                                    <tr class="border-black o_subtotal">
                                        <td>
                                            <strong>Descuento Total</strong>
                                        </td>
                                        <td class="text-right">
                                            <span t-att-class="oe_subtotal_footer_separator" t-field="o.discount_total"/>
                                        </td>
                                    </tr>
                                    <tr class="border-black o_subtotal">
                                        <td>
                                            <strong>SubTotal</strong>
                                        </td>
                                        <td class="text-right">
                                            <span t-att-class="oe_subtotal_footer_separator" t-esc="subtotal['formatted_amount']"/>
                                        </td>
                                    </tr>
                                    <t t-set="subtotal_to_show" t-value="subtotal['name']"/>
                                    <t t-foreach="tax_totals['groups_by_subtotal'][subtotal_to_show]" t-as="amount_by_group">
                                        <tr>
                                            <t t-if="len(tax_totals['groups_by_subtotal'][subtotal_to_show]) &gt; 1">
                                                <td>
                                                    <span t-esc="amount_by_group['tax_group_name']"/>
                                                    <span class="text-nowrap">on
                                                        <t t-esc="amount_by_group['formatted_tax_group_base_amount']"/>
                                                    </span>
                                                </td>
                                                <td class="text-right o_price_total">
                                                    <span class="text-nowrap" t-esc="amount_by_group['formatted_tax_group_amount']"/>
                                                </td>
                                            </t>
                                            <t t-else="">
                                                <td>
                                                    <span class="text-nowrap" t-esc="amount_by_group['tax_group_name']"/>
                                                </td>
                                                <td class="text-right o_price_total">
                                                    <span class="text-nowrap" t-esc="amount_by_group['formatted_tax_group_amount']"/>
                                                </td>
                                            </t>
                                        </tr>
                                    </t>
                                </t>

                                <!--Total amount with all taxes-->
                                <tr class="border-black o_total">
                                    <td>
                                        <strong>Total</strong>
                                    </td>
                                    <td class="text-right">
                                        <span t-esc="tax_totals['formatted_amount_total']"/>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-7" style="font-size:16px">
                        <t t-if="o.company_id.country_id.code=='AR'">
                            <span style="font-size:12px;color:black;">
                                <t t-if="o.l10n_ar_afip_concept in ['2', '3', '4'] and o.l10n_ar_afip_service_start and o.l10n_ar_afip_service_end">
                                    <strong>Periodo facturado:</strong>
                                    <span t-field="o.l10n_ar_afip_service_start"/>
                                    a
                                    <span t-field="o.l10n_ar_afip_service_end"/>
                                </t>
                            </span>
                        </t>
                    </div>
                    <br/>
                    <div class="col-1"/>
                    <div class="col-4" style="font-size:12px">
                         <t t-if="o.ei_is_valid">
                             <span>
                                    <strong>
                                        <span>Método de pago: </span>
                                    </strong>
                                    <t t-esc="o.payment_form_id.name"/>
                                </span>
                         </t>
                        <br/>
                        <span>
                            <span>
                                <strong>Valor en letras:</strong>
                                <t t-esc="o.invoice_amount_text"/>
                            </span>

                        </span>

                    </div>
                </div>

                <p t-if="o.move_type in ('out_invoice', 'in_refund') and o.payment_reference" name="payment_communication" style="font-size:14px">
                    Utilize la siguiente referencia al realizar su pago :
                    <b>
                        <span t-field="o.payment_reference"/>
                    </b>
                </p>
                <div name="comment" position="before">
                    <p t-if="o.journal_id.l10n_ar_afip_ws == 'wsbfe'">
                        THE PURCHASER ASSUMES THE COMMITMENT, IN THE NATURE OF AN AFFECTED DECLARATION, OF NOT EXPORTING
                        THE PROPERTY DURING THE TERM OF THREE 3 YEARS FROM THE ACQUISITION
                        <br/>
                    </p>
                </div>

                <!-- footer data -->
                <div style="float:right;">
                    <t t-if="o.company_id.country_id.code=='AR'" style="float:right;">
                        <div name="footer" position="inside">
                            <div name="footer_left_column" position="inside">
                                <img t-if="o.l10n_ar_afip_qr_code" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('QR', o.l10n_ar_afip_qr_code, 400, 400)" alt="QR Code" style="height:100px"/>
                            </div>
                            <div name="pager" position="before">
                                <t t-if="o.journal_id.l10n_ar_afip_pos_system and o.journal_id.l10n_ar_afip_pos_system != 'II_IM'">
                                    <div>
                                        <t t-if="o.l10n_ar_afip_auth_code">
                                            <span style="font-size:12px">
                                                <strong>CAE:</strong>
                                                <t t-esc="o.l10n_ar_afip_auth_code"/>
                                            </span>
                                        </t>
                                        <t t-if="not o.l10n_ar_afip_auth_code">
                                            <strong style="font-size:12px;">CAE: no disponible</strong>
                                        </t>
                                    </div>
                                    <div>
                                        <span style="font-size:12px">
                                            <strong>Fecha Vto. CAE:</strong>
                                            <t t-if="o.l10n_ar_afip_auth_code_due">
                                                <t t-esc="o.l10n_ar_afip_auth_code_due" t-field-options="{&quot;format&quot;: &quot;dd/MM/yyyy&quot;}"/>
                                            </t>
                                            <t t-if="not o.l10n_ar_afip_auth_code_due">
                                                Fecha no disponible
                                            </t>
                                        </span>
                                    </div>
                                </t>
                            </div>
                        </div>
                    </t>
                </div>


                <t t-if="o.company_id.country_id.code=='CR'">
                    <p class="text-left" style="padding:2px 2px 2px 2px; font-size:14px " t-if="o.move_type in ('out_invoice', 'in_refund') and o.number_electronic">
                        Clave:<span t-field="o.number_electronic"/>&amp;nbsp;&amp;nbsp;&amp;nbsp;// Usuario:
                        <span t-field="o.user_id.name"/>
                        <br/>
                        Autorizada mediante resolución No DGT-R-033-2019 del 20 de junio de 2019
                    </p>
                </t>
                <p t-if="o.invoice_payment_term_id" name="payment_term" style="font-size:14px">
                    <span t-field="o.invoice_payment_term_id.note"/>
                </p>
                <div t-if="not is_html_empty(o.narration)" name="comment">
                    <span t-field="o.narration" style="font-size:12px"/>
                </div>
                <p t-if="not is_html_empty(o.fiscal_position_id.note)" name="note">
                    <span t-field="o.fiscal_position_id.note" style="font-size:12px"/>
                </p>
                <p t-if="o.invoice_incoterm_id" name="incoterm">
                    <strong>Incoterm:</strong>
                    <span t-field="o.invoice_incoterm_id.code"/>
                    -
                    <span t-field="o.invoice_incoterm_id.name"/>
                </p>
                <t t-if="o.company_id.country_id.code=='CR'">
                    <div id="qrcode" t-if="o.display_qr_code and o.amount_residual &gt; 0">
                        <p t-if="qr_code_urls.get(o.id)">
                            <strong class="text-center">Scan me with your banking app.</strong>
                            <br/>
                            <br/>
                            <img class="border border-dark rounded" t-att-src="qr_code_urls[o.id]"/>
                        </p>
                    </div>
                </t>
                <!-- chile -->
                <div name="stamp" class="col-4 text-center">
                    <t t-if="o.l10n_cl_sii_barcode">
                        <t t-set="barcode_stamp" t-value="o._pdf417_barcode(o.l10n_cl_sii_barcode)"/>
                        <t t-if="barcode_stamp">
                            <img class="img-fluid" t-attf-src="data:image/*;base64,{{barcode_stamp}}"/>
                            <p t-att-style="'color: %s;' % o.company_id.primary_color" class="text-center small">
                                Timbre Electrónico SII
                                <br/>
                                Resolución Nº:
                                <span t-field="o.company_id.l10n_cl_dte_resolution_number"/>
                                de Fecha:
                                <span t-field="o.company_id.l10n_cl_dte_resolution_date" t-field-options="{&quot;widget&quot;: &quot;date&quot;}"/>
                                <span name="verification_url">Verifique documento en www.sii.cl</span>
                            </p>
                        </t>
                    </t>
                </div>
                <!-- Is there a discount on at least one line? -->
                <t t-set="display_discount" t-value="any([l.discount for l in o.invoice_line_ids])"/>
                <t t-set="amount_total" t-value="0.0"/>
                <t t-set="amount_untaxed" t-value="0.0"/>


                <!--INICIA TABLA-->

                <!--TERMINA TABLA DE INFORMACIÓN CENTRAL-->

                <!--INICIAN ELEMENTOS DEL SAT-->
                <!--CORRECCIÓN DE ETIQUETAS STRONG-->
            </div>
        </t>
    </t>

