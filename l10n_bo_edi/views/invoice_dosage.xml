<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <!-- Selling Point List -->
        <record model="ir.ui.view" id="invoice_dosage_view_list">
            <field name="name">selling.point.view.list</field>
            <field name="model">invoice_dosage</field>
            <field name="arch" type="xml">
                <tree string="Invoice Dosage">
                    <field name="code"/>
                    <field name="auth_number"/>
                    <field name="end_date"/>
                    <field name="invoice_number"/>
                </tree>
            </field>
        </record>

        <!-- Selling point Search -->
        <record model="ir.ui.view" id="invoice_dosage_view_search">
            <field name="name">selling.point.view.search</field>
            <field name="model">invoice_dosage</field>
            <field name="arch" type="xml">
                <search>
                    <field name="code"/>
                    <field name="auth_number"/>
                    <field name="end_date"/>
                    <field name="invoice_number"/>
                </search>
            </field>
        </record>


    </data>


</odoo>
