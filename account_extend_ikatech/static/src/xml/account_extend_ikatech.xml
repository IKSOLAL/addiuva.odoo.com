      <!--? Filters Section View -->
      <t t-name="analyticPandlFilterView">
        <div class="pandl_filter_fiew">
          <div>
            <!-- <span><input type="number" id="months"/></span> -->
          </div>
          <div class="box-comparation">
            <span class="fa fa-bar-chart color-btn-sex icon_comparation"></span>
            <span class="checkbox-inline check-inline color-btn-sex cursorPointer"  id="comparation_button">Comparación:</span>
            <input type="checkbox" id="comparation"  hidden="hidden"/>
            <div class="box-months" id="boxComparationCont" hidden="hidden">
                <a class="cursorPointer btn-comparation" id="monthComparation">Meses Anteriores</a>
                <div class="box-inside" id="months_box_comparation" hidden="hidden">
                  <span>No. Meses:</span>
                  <input class="box-number" type="number" id="months" value="0" min="0" max="12"/>
                </div>
                <a class="cursorPointer btn-comparation" id="yearsComparation">Años Anteriores</a>
                <div class="box-inside"  id="year_box_comparation" hidden="hidden">
                  <span>No. Años:</span>
                  <input class="box-number" type="number" id="years" value="0" min="0" max="12"/>
                </div>
                <a class="cursorPointer btn-comparation" id="monthYearComparation">Mes Años Anteriores</a>
                <div class="box-inside"  id="year_month_box_comparation" hidden="hidden">
                  <span>No. Meses:</span>
                  <input class="box-number" type="number" id="monthYear" value="0" min="0" max="12"/>
                </div>
                <!--  -->
                <a class="cursorPointer btn-comparation" id="datesComparation">Fecha Personalizada</a>
                <div class="box-inside" style="flex-direction: column" id="dates_box_comparation" hidden="hidden">
                    <div class="comparation-dates-input">
                      <span>Desde:</span>
                      <input type="date" id="comparation_date_to"
                        class="form-control datetimepicker-input headerZise"></input>
                    </div>
                    <div class="comparation-dates-input">
                      <span>Hasta:</span>
                      <input type="date" id="comparation_date_from"
                        class="form-control datetimepicker-input headerZise"></input>
                    </div>
                </div>
            </div>
          </div>
          <div class="box-dates">
              <span class="fa fa-calendar color-btn-sex icon_comparation"></span>
              <span class="checkbox-inline check-inline color-btn-sex cursorPointer" id="setDates">Fechas:</span>
              <div class="box-dates-set" id="boxSetDates" hidden="hidden">
                  <a class="cursorPointer btn-comparation btn-dates" id="thisMonth">Este mes</a>
                  <a class="cursorPointer btn-comparation btn-dates"  id="thisSemester">Este trimestre</a>
                  <a class="cursorPointer btn-comparation btn-dates" id="thisYear"> <i id="checkDate" class="fa fa-solid fa-check" style="color: #28a745;"/>Este año fiscal</a> 
                  <a class="cursorPointer btn-comparation btn-dates divider_line"></a>
                  <a class="cursorPointer btn-comparation btn-dates" id="lastMonth">El mes pasado</a>
                  <a class="cursorPointer btn-comparation btn-dates" id="lastSemester">Trimestre anterior</a>
                  <a class="cursorPointer btn-comparation btn-dates" id="lastYear">Año fiscal anterior</a>
                  <a class="cursorPointer btn-comparation btn-dates divider_line"></a>
                  <a class="cursorPointer btn-comparation btn-dates" id="date_custom">Personalizado</a>
                  <div class="box-inside-dates" hidden="hidden">
                    <div class="comparation-dates-input">
                      <span>Desde:</span>
                      <input type="date" name="date_to"
                        class="form-control datetimepicker-input headerZise"
                        data-target="#date_to"
                        t-att-name="prefix"></input>
                    </div>
                    <div class="comparation-dates-input">
                      <span>Hasta:</span>
                      <input type="date" name="date_from"
                        class="form-control datetimepicker-input headerZise"
                        data-target="#date_from"
                        t-att-name="prefix"></input>
                    </div>
                </div>
              </div>
          </div>
          <div style="margin-left:5px">
              <button type="button" id="apply_filter" class="btn btn-primary btnHeader">Aplicar</button>
          </div>
        </div>
        <div class="pandl_filter_fiew_left">
           <div style="margin-left:5px">
              <button type="button" id="generate_xlsx" class="btn btn-primary btn_xlsx">Excel</button>
               <button type="button" id="generate_pdf" class="btn btn-primary btn_pdf">PDF</button>
          </div>
        </div>
      </t>
